---
import { Image } from 'astro:assets';
import logoipsum from "../assets/logoipsum-296.svg";
---



<nav class="bg-white shadow-md top-0 z-[9999] w-full">
    <div class="container mx-auto px-6 py-3 flex items-center justify-between">
      <a href="#" class="flex items-center space-x-2 focus:outline-none focus:ring-2 focus:ring-blue-500 rounded-md">
        <Image src={logoipsum} alt="logo" class="w-10 h-10 rounded-full ml-0 lg:ml-25">
      </a>
  
      <div class="hidden lg:flex lg:items-center lg:space-x-6">
        <a href="/AstroTemplate" class="text-gray-700 hover:text-blue-500 transition-colors duration-200 focus:outline-none focus:ring-2 focus:ring-blue-500 rounded-md">Homepage</a>
  
        <!-- Services Dropdown -->
        <div class="relative group">
          <button aria-label="Services dropdown" class="flex items-center text-gray-700 hover:text-blue-500 focus:outline-none focus:ring-2 focus:ring-blue-500 rounded-md group-hover:text-blue-500 transition-colors duration-200">
            Services
            <svg class="h-5 w-5 ml-1 group-hover:rotate-180 transition-transform duration-200" viewBox="0 0 20 20" fill="currentColor">
                <path fill-rule="evenodd" d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z" clip-rule="evenodd" />
              </svg>
          </button>
          <ul class="absolute hidden group-hover:block bg-white border border-gray-200 rounded shadow-md mt-0 w-64 z-50">
            <li><a href="/AstroTemplate/ServiceHighlightOne" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">AC Repair & Install</a></li>
            <li><a href="/AstroTemplate/ServiceHighlightTwo" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Furnace & Heater Services</a></li>
            <li><a href="/AstroTemplate/ServiceHighlightThree" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Indoor Air Quality Services</a></li>
            <li><a href="/AstroTemplate/ServiceHighlightFour" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">HVAC Maintenance & Tune-Ups</a></li>
            <li><a href="/AstroTemplate/ServiceHighlightFive" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Smart Thermostat Installation</a></li>
            <li><a href="/AstroTemplate/ServiceHighlightSix" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Residential & Commercial Services</a></li>
          </ul>
        </div>
  
        <!-- Locations Dropdown -->
        <div class="relative group">
          <button aria-label="Locations dropdown" class="flex items-center text-gray-700 hover:text-blue-500 focus:outline-none focus:ring-2 focus:ring-blue-500 rounded-md group-hover:text-blue-500 transition-colors duration-200">
            Locations
            <svg class="h-5 w-5 ml-1 group-hover:rotate-180 transition-transform duration-200" viewBox="0 0 20 20" fill="currentColor">
              <path fill-rule="evenodd" d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z" clip-rule="evenodd" />
            </svg>
          </button>
          <ul class="absolute hidden group-hover:block bg-white border border-gray-200 rounded shadow-md mt-0 w-64 z-50 max-h-80 overflow-y-auto">
            <li><a href="/AstroTemplate/LocationOne" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Port Richey, FL</a></li>
            <li><a href="/AstroTemplate/LocationTwo" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">New Port Richey, FL</a></li>
            <li><a href="/AstroTemplate/LocationThree" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Trinity, FL</a></li>
            <li><a href="/AstroTemplate/LocationFour" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Tarpon Springs, FL</a></li>
            <li><a href="/AstroTemplate/LocationFive" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Oldsmar, FL</a></li>
            <li><a href="/AstroTemplate/LocationSix" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Odessa, FL</a></li>
            <li><a href="/AstroTemplate/LocationSeven" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Land O' Lakes, FL</a></li>
            <li><a href="/AstroTemplate/LocationEight" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Spring Hill, FL</a></li>
            <li><a href="/AstroTemplate/LocationNine" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Palm Harbor, FL</a></li>
            <li><a href="/AstroTemplate/LocationTen" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Dunedin, FL</a></li>
            <li><a href="/AstroTemplate/LocationEleven" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Holiday, FL</a></li>
            <li><a href="/AstroTemplate/LocationTwelve" class="block px-4 py-2 hover:bg-gray-100 text-gray-700">Hudson, FL</a></li>
          </ul>
        </div>
  
        <a href="/AstroTemplate/aboutus" class="text-gray-700 hover:text-blue-500 transition-colors duration-200 focus:outline-none focus:ring-2 focus:ring-blue-500 rounded-md">About Us</a>
        <a href="#" class="text-gray-700 hover:text-blue-500 transition-colors duration-200 focus:outline-none focus:ring-2 focus:ring-blue-500 rounded-md">Blog</a>
        <a href="/schedule" class="bg-red-600 hover:bg-red-700 text-white py-2 px-4 rounded ...">Schedule Online</a>
        <a href="#" class="bg-blue-500 hover:bg-blue-600 text-white py-2 px-4 rounded focus:outline-none focus:ring-2 focus:ring-blue-500 mr-25">111-222-3333</a>
      </div>
  
      <!-- Mobile Toggle -->
      <div class="lg:hidden flex items-center">
        <button aria-label="toggle menu button" class="p-2 text-gray-500 focus:outline-none focus:ring-2 focus:ring-blue-500 rounded-md toggle-menu-button">
          <svg class="h-6 w-6 fill-current" viewBox="0 0 24 24">
            <path fill="currentColor" d="M4 6h16v2H4V6zm0 5h16v2H4v-2zm0 5h16v2H4v-2z" />
          </svg>
        </button>
      </div>
    </div>
  
 <!-- Mobile Menu -->
<div class="mobile-menu hidden absolute top-full left-0 w-full bg-white border-b border-gray-200 shadow-md text-2xl">
  <div class="flex flex-col p-4 space-y-8 text-center">
    <a href="/AstroTemplate" class="text-gray-700 hover:text-blue-500 focus:outline-none focus:ring-2 focus:ring-blue-500 inline-block mx-auto">Homepage</a>

    <!-- Services Mobile -->
    <details class="group">
      <summary class="cursor-pointer flex justify-center items-center text-gray-700 hover:text-blue-500 focus:outline-none focus:ring-2 focus:ring-blue-500">Services</summary>
      <ul class="mt-2 space-y-12 text-center text-xl bg-blue-50">
        <li><a href="/AstroTemplate/ServiceHighlightOne" class="inline-block mx-auto">AC Repair & Install</a></li>
        <li><a href="/AstroTemplate/ServiceHighlightTwo" class="inline-block mx-auto">Furnace & Heater Services</a></li>
        <li><a href="/AstroTemplate/ServiceHighlightThree" class="inline-block mx-auto">Indoor Air Quality Services</a></li>
        <li><a href="/AstroTemplate/ServiceHighlightFour" class="inline-block mx-auto">HVAC Maintenance & Tune-Ups</a></li>
        <li><a href="/AstroTemplate/ServiceHighlightFive" class="inline-block mx-auto">Smart Thermostat Installation</a></li>
        <li><a href="/AstroTemplate/ServiceHighlightSix" class="inline-block mx-auto">Residential & Commercial Services</a></li>
      </ul>
    </details>

    <!-- Locations Mobile -->
    <details class="group">
      <summary class="cursor-pointer flex justify-center items-center text-gray-700 hover:text-blue-500 focus:outline-none focus:ring-2 focus:ring-blue-500">Locations</summary>
      <ul class="mt-2 space-y-8 text-center text-xl bg-blue-50">
        <li><a href="/AstroTemplate/LocationOne" class="inline-block mx-auto">Port Richey, FL</a></li>
        <li><a href="/AstroTemplate/LocationTwo" class="inline-block mx-auto">New Port Richey, FL</a></li>
        <li><a href="/AstroTemplate/LocationThree" class="inline-block mx-auto">Trinity, FL</a></li>
        <li><a href="/AstroTemplate/LocationFour" class="inline-block mx-auto">Tarpon Springs, FL</a></li>
        <li><a href="/AstroTemplate/LocationFive" class="inline-block mx-auto">Oldsmar, FL</a></li>
        <li><a href="/AstroTemplate/LocationSix" class="inline-block mx-auto">Odessa, FL</a></li>
        <li><a href="/AstroTemplate/LocationSeven" class="inline-block mx-auto">Land O' Lakes, FL</a></li>
        <li><a href="/AstroTemplate/LocationEight" class="inline-block mx-auto">Spring Hill, FL</a></li>
        <li><a href="/AstroTemplate/LocationNine" class="inline-block mx-auto">Palm Harbor, FL</a></li>
        <li><a href="/AstroTemplate/LocationTen" class="inline-block mx-auto">Dunedin, FL</a></li>
        <li><a href="/AstroTemplate/LocationEleven" class="inline-block mx-auto">Holiday, FL</a></li>
        <li><a href="/AstroTemplate/LocationTwelve" class="inline-block mx-auto">Hudson, FL</a></li>
      </ul>
    </details>

    <a href="/AstroTemplate/aboutus" class="text-gray-700 hover:text-blue-500 inline-block mx-auto">About Us</a>
    <a href="#" class="text-gray-700 hover:text-blue-500 inline-block mx-auto">Blog</a>
    <a href="/schedule" class="bg-red-600 hover:bg-red-700 text-white py-2 px-4 rounded inline-block mx-auto">Schedule Online</a>
    <a href="#" class="bg-blue-500 hover:bg-blue-600 text-white py-2 px-4 rounded inline-block mx-auto">111-2222-33333</a>
  </div>
</div>



  </nav>
  <script type="module">
    document.querySelector('.toggle-menu-button')?.addEventListener('click', () => {
      document.querySelector('.mobile-menu')?.classList.toggle('hidden');
    });
  </script>